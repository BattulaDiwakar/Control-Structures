import java.util.Scanner;
public class Harshard
{
    public static void main(String[] args)
    {Scanner sc=new Scanner(System.in);
    int rem,sum=0,x;
    int a=sc.nextInt(); //1729
    x=a;
    while(a>0)
    {rem=a%10;
    a=a/10;
    sum=sum+rem;
    }
    //System.out.println(a+ " " + sum); //1729 19
    if(x%sum==0)
    System.out.println("Harshad Number");
    else //if(a%sum!=0)
    System.out.println("Not Harshad Number");//iam going to be rock woohoooooo!!!!!!
    }
}
