import java.util.Scanner;
public class Sum
{
 public static void main(String args[])
    {Scanner sc=new Scanner(System.in);
    int rem,sum=0;
    int num=sc.nextInt();
    while(num>0)
    {rem=num%10;
    num=num/10;
    sum=sum+rem;
     
    }System.out.println(""+sum);
        
    
       
    }
}
