import java.util.Scanner;
public class Assessment2
{public static void main(String args[])
{    Scanner sc=new Scanner(System.in);
    int a,b,c;
    a = sc.nextInt();
    b =sc.nextInt();
    c =sc.nextInt();
    float d,e,f,g;
    d =(a/100)*c;
    e =a+d;
    f =(2*d)/100;
    g =e-f;
    System.out.println(d);
    System.out.println(e);
    System.out.println(f);
    System.out.println(g);
}
}
